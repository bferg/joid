    SINGLE-VALUE )

objectclass ( 1.3.6.1.4.1.18060.123.10
    NAME 'opAssociationOc'
    DESC 'OpenID Association objectclass'
    SUP top
    STRUCTURAL
    MUST ( opModeAt $ opHandleAt $ opSecretAt $ 
           opIssuedDateAt $ opLifetimeAt $ opAssociationType ) )

