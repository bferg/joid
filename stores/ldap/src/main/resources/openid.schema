    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
    SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.18060.123.7
    NAME 'opAssociationTypeAt'
    DESC 'OpenID Association type attribute'
    EQUALITY exactCaseMatch
    SUBSTRING exactCaseSubstringsMatch
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
